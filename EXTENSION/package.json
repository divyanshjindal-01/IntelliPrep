{
    "name": "divyanshudebug-buddy",
    "displayName": "Debug Buddy",
    "description": "A VS Code extension to analyze and fix code using an AI backend.",
    "version": "0.0.1",
    "main": "./extension.js",
    "engines": {
        "vscode": "^1.103.0"
    },
    "categories": [
        "Other"
    ],
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": true
        },
        "virtualWorkspaces": {
            "supported": true
        },
        "authentication": {
            "providers": [
                "github"
            ]
        }
    },
    "activationEvents": [
        "onCommand:divyanshudebug-buddy.analyzeAndFix",
        "onCommand:divyanshudebug-buddy.login",
        "onCommand:divyanshudebug-buddy.logout"
    ],
    "contributes": {
        "commands": [
            {
                "command": "divyanshudebug-buddy.analyzeAndFix",
                "title": "Debug Buddy: Analyze & Fix Code ðŸ¤–"
            },
            {
                "command": "divyanshudebug-buddy.login",
                "title": "Debug Buddy: Login (GitHub) ðŸ”’"
            },
            {
                "command": "divyanshudebug-buddy.logout",
                "title": "Debug Buddy: Logout ðŸ‘‹"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "divyanshudebug-buddy.analyzeAndFix",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.10",
        "@types/node": "^22.18.12",
        "@types/socket.io-client": "^1.4.36",
        "@types/vscode": "^1.105.0",
        "@vscode/test-cli": "^0.0.11",
        "@vscode/test-electron": "^2.5.2",
        "eslint": "^9.32.0"
    },
    "dependencies": {
        "debug": "^4.4.3",
        "engine.io": "^6.6.4",
        "firebase": "^12.4.0",
        "firebase-admin": "^13.5.0",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1",
        "uuid": "^9.0.1"
    }
}
